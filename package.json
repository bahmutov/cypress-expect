{
  "name": "cypress-expect",
  "version": "0.0.0-development",
  "description": "Cypress CLI wrapper where you can specify the total number of expected tests",
  "bin": {
    "cypress-expect": "index.js"
  },
  "files": [
    "index.js"
  ],
  "scripts": {
    "test": "node . run --passing 3",
    "semantic-release": "semantic-release",
    "test:expect:1": "node . run --expect ./cypress/integration/expected.json",
    "test:expect:2": "node . run --expect ./cypress/failing/expected.json --config integrationFolder=cypress/failing"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bahmutov/cypress-expect.git"
  },
  "keywords": [
    "cypress",
    "cli"
  ],
  "author": "Gleb Bahmutov <gleb.bahmutov@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bahmutov/cypress-expect/issues"
  },
  "homepage": "https://github.com/bahmutov/cypress-expect#readme",
  "devDependencies": {
    "@types/node": "14.10.1",
    "cypress": "7.2.0",
    "prettier": "2.1.1",
    "semantic-release": "^17.1.1"
  },
  "dependencies": {
    "arg": "4.1.3",
    "debug": "4.3.1"
  },
  "peerDependencies": {
    "cypress": ">=5.3.0"
  },
  "release": {
    "branches": "main"
  }
}
